{
  "id": "default",
  "description": "Demand Prediction for NYC Taxi",
  "engineFactory": "edu.cs5152.predictionio.demandforecasting.ForecastingEngine",
  "datasource": {
    "params": {
      "appName": "koober"
    }
  },
  "algorithms": [
      {
        "name": "algGBTree",
        "params": {
        "iterations": 100,
        "maxDepth": 10
        }
      }
      ,
      {
        "name": "algRegression",
        "params": {
        "iterations": 500,
        "miniBatchFraction" : 0.5,
        "regParam" : 0.0,
        "stepSize": 0.01
        }
      },
      {
        "name": "multinomialLogistic",
        "params": {
        "iterations": 4096,
        "numClasses" : 200,
        "regParam" : 0.5,
        "convergenceTol": 0.01
        }
      },
      {
        "name": "randomForest",
        "params": {
          "numTrees": 10,
          "maxDepth": 10,
          "numBins": 32
        }
      }
  ]
}

